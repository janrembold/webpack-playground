    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "fs": "^0.0.1-security",
    "glob": "^7.1.2",
    "json-server": "^0.10.1",
    "parallelshell": "^3.0.1",
    "path": "^0.12.7",
